# LitGPT Light Compute Project - Secrets Template
# Copy this file to secrets.env and fill in your actual credentials

# GitHub credentials for repository access
GITHUB_USER=your_github_username
GITHUB_PAT=your_github_personal_access_token

# RunPod API key for GPU deployment
RUNPOD_API_KEY=your_runpod_api_key

# Maximum GPU cost in USD (default: $20)
MAX_GPU_COST=20.0 